<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.cs.dao.compcondition.CompConditionMapper" >
  <resultMap id="BaseResultMap" type="com.cs.pojo.CompCondition" >
    <result column="proId" property="proid" jdbcType="INTEGER" />
    <result column="conditionId" property="conditionid" jdbcType="INTEGER" />
  </resultMap>
  <insert id="insert" parameterType="com.cs.pojo.CompCondition" >
    insert into compcondition (proId, conditionId)
    values (#{proid,jdbcType=INTEGER}, #{conditionid,jdbcType=INTEGER})
  </insert>
  <insert id="insertSelective" parameterType="com.cs.pojo.CompCondition" >
    insert into compcondition
    <trim prefix="(" suffix=")" suffixOverrides="," >
      <if test="proid != null" >
        proId,
      </if>
      <if test="conditionid != null" >
        conditionId,
      </if>
    </trim>
    <trim prefix="values (" suffix=")" suffixOverrides="," >
      <if test="proid != null" >
        #{proid,jdbcType=INTEGER},
      </if>
      <if test="conditionid != null" >
        #{conditionid,jdbcType=INTEGER},
      </if>
    </trim>
  </insert>
</mapper>